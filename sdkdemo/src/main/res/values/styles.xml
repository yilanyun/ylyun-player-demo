<resources>

    <style name="CustomWindowTitleBackground">
        <item name="android:background">#ffffff</item>
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar"/>

    <!--没有选中的样式-->
    <style name="bottom_normal_text">
        <item name="android:textColor">#FF979797</item>
        <item name="android:textSize">12sp</item>
    </style>
    <!--选中的样式-->
    <style name="bottom_selected_text">
        <item name="android:textColor">#FF3B98FC</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="FloatWindowAnimation">
        <item name="android:windowEnterAnimation">@anim/anim_float_window_enter</item>
        <item name="android:windowExitAnimation">@anim/anim_float_window_exit</item>
    </style>

</resources>
